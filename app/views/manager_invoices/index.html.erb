<%= render 'layouts/interpreter_or_manager_or_customer_dashboard' %>
<div id="page-wrapper">
  <%= render 'layouts/alerts' %>
  <div class="row">
    <div class="col-lg-12">
      <h3 class="page-header">
        <% if user_logged_in? && current_user.manager? %>
          <% provide(:title, 'All Manager Invoices') %>
          All Manager Invoices
        <% elsif user_logged_in? %>
          <% provide(:title, 'TSHA Invoices') %>
          TSHA Invoices
        <% else %>
          <% provide(:title, 'TSHA Invoices') %>
          Your Invoices
        <% end %>
      </h3>
      <!-- /.page-header -->
    </div>
    <!-- /.col-lg-12 -->
  </div>
  <!-- /.row -->
  <% if user_logged_in? && current_user.manager? %>
    <%= render 'manager_invoice_search' %>
  <% end %>
  <%= render 'manager_invoices_table' %>
</div>
<!-- /.page-wrapper -->
