<% if @job_requests.count == 0 %>
  <%= render 'layouts/no_results_found_alert' %>
<% else %>
  <table class="table table-striped table-hover">
    <thead>
      <tr>
        <th>
          ID
        </th>
        <th>
          Deaf Client
        </th>
        <th>
          Contact Person
        </th>
        <th>
          Start Date
        </th>
        <th>
          Status
        </th>
      </tr>
    </thead>
    <tbody>
      <%= render @job_requests %>
    </tbody>
  </table>
  <!-- /.table table-striped table-hover -->
<% end %>
