<% if @deaf_clients.count == 0 %>
  <%= render 'layouts/no_results_found_alert' %>
<% else %>
  <table class="table table-striped table-hover">
    <thead>
      <tr>
        <th>
          First Name
        </th>
        <th>
          Last Name
        </th>
      </tr>
    </thead>
    <tbody>
      <%= render @deaf_clients %>
    </tbody>
  </table>
  <!-- /.table table-striped -->
<% end %>
